import java.util.*;

public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
         Scanner s = new Scanner(System.in);
                 char c = s.next().charAt(0); 
            if(c >= 'a' && c <= 'z')
           {
               System.out.print("0");
           }
        else if(c >= 'A' && c <= 'Z')
        {
            System.out.print("1");
        }
        else
        {
            System.out.print("-1");
        }

    }
}
