import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        // Write your code here
        Scanner sc =new Scanner(System.in);
        int n=sc.nextInt();
		int b=0;
        for(int i=1;b<n;i++){
            
            int sum=3*i+2;
            if(sum%4!=0){
                System.out.print(sum +" ");
                b++;
            } 
        }
    }
}
