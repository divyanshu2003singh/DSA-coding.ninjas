import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
         int count=0; 
        int d=0;
         while(n>0){
            int digit=n%10;
             n=n/10;
             d= d+digit*(int)Math.pow(2,count);
             count++;
        }
        System.out.println(d);
    }}
