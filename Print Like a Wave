
public class Solution {

	public static void wavePrint(int mat[][]){
		//Your code goes here
		int n=mat.length;
		int m=mat[0].length;
		for(int j=0;j<m;j++){
			if(j%2==0){
				for(int i=0;i<n;i++){
				System.out.print(mat[i][j]+" ");
				}
			}
			else{
				for(int i=m-1;i>=0;i--){
					System.out.print(mat[i][j]+" ");
				}
			}
		}
	}

}
