public class Solution {  

    public static void pushZerosAtEnd(int[] arr) {
    	//Your code goes here
            int temp=0;
        int n=arr.length;
        for(int i= 0; i<n ; i++){
            if(arr[i]!=0){
                arr[temp++]=arr[i];
            } 
            }
             while(temp<n){
               arr[temp++]=0;
            }
                
        }
        } 
    

