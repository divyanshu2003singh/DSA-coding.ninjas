
public class Solution {

	public static String removeConsecutiveDuplicates(String str) {
		//Your code goes here
		int n=str.length();
		String res="";
		for(int i=0;i<n;i++){
			if( i<n-1 && str.charAt(i)==str.charAt(i+1)){
				continue;
			}
			else{
				res+=str.charAt(i);
			}
		}
		return res;
	}

}
