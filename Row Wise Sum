
public class Solution {

	public static void rowWiseSum(int[][] mat) {
		//Your code goes here
		int m=mat.length;
		int n=mat[0].length;
		if(m==0 )//&&n==0) 
		{
			//System.out.print("0");
			return;
		}
		for(int i=0;i<m;i++){
			int sum= 0;
			for(int j=0;j<n;j++){
              sum=sum+mat[i][j];
			}
			System.out.print(sum+ " ");
		}
	}

}
